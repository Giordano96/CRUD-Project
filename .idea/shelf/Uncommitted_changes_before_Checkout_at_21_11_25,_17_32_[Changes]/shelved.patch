+# filename: save_missing_ingredients_to_txt.py
+
+import csv
+import re
+from collections import Counter
+from datetime import datetime
+
+CSV_FILE = "recipes_cleaned.csv"
+OUTPUT_FILE = "ingredienti_mancanti.txt"
+
+print("Analisi in corso... salvataggio in corso su ingredienti_mancanti.txt\n")
+
+missing_counter = Counter()
+recipes_with_missing = 0
+total_recipes = 0
+results = []
+
+# Intestazione del file di output
+results.append("=" * 90)
+results.append("INGREDIENTI MANCANTI IN ingredients_parsed MA PRESENTI IN tags")
+results.append(f"Generato il: {datetime.now().strftime('%d/%m/%Y %H:%M:%S')}")
+results.append(f"File analizzato: {CSV_FILE}")
+results.append("=" * 90)
+results.append("")
+
+with open(CSV_FILE, "r", encoding="utf-8") as f:
+    reader = csv.DictReader(f)
+
+    for row in reader:
+        total_recipes += 1
+        recipe_name = row["recipe_name"].strip()
+        tags_str = row["tags"].strip()
+        ingredients_parsed = row["ingredients_parsed"]
+
+        if not tags_str:
+            continue
+
+        # Estrai tag
+        tags = {t.strip().lower() for t in tags_str.split(",") if t.strip()}
+
+        # Estrai ingredienti da ingredients_parsed
+        parsed_ingredients = {
+            match.strip().lower()
+            for match in re.findall(r"ingredient:\s*(.*?)(?=quantity:|$)", ingredients_parsed, re.IGNORECASE)
+            if match.strip()
+        }
+
+        # Trova mancanti
+        missing_in_recipe = []
+        for tag in tags:
+            tag_clean = tag.lower()
+            found = any(
+                tag_clean in parsed.lower() or parsed.lower() in tag_clean
+                for parsed in parsed_ingredients
+            )
+            if not found:
+                missing_in_recipe.append(tag)
+                missing_counter[tag] += 1
+
+        if missing_in_recipe:
+            recipes_with_missing += 1
+            results.append(f"RICETTA: {recipe_name}")
+            results.append(f"   → Ingredienti mancanti in ingredients_parsed: {', '.join(sorted(missing_in_recipe))}")
+            results.append("-" * 90)
+
+# === RIEPILOGO FINALE ===
+results.append("")
+results.append("RIEPILOGO FINALE")
+results.append("=" * 90)
+results.append(f"Ricette totali analizzate:               {total_recipes}")
+results.append(f"Ricette con ingredienti mancanti:        {recipes_with_missing}")
+results.append(f"Percentuale con problemi:                 {recipes_with_missing / total_recipes * 100:.1f}%")
+results.append("")
+results.append("TOP 30 INGREDIENTI PIÙ FREQUENTEMENTE MANCANTI")
+results.append("-" * 70)
+for ingredient, count in missing_counter.most_common(30):
+    results.append(f"{ingredient:30} → {count:4} volte")
+
+results.append("")
+results.append(f"Totale ingredienti unici mancanti: {len(missing_counter)}")
+results.append("")
+results.append("Fine del report.")
+
+# === SALVA SU FILE ===
+with open(OUTPUT_FILE, "w", encoding="utf-8") as f:
+    f.write("\n".join(results))
+
+print(f"Analisi completata!")
+print(f"Risultati salvati in: {OUTPUT_FILE}")
+print(
+    f"→ {recipes_with_missing} ricette su {total_recipes} hanno ingredienti mancanti ({recipes_with_missing / total_recipes * 100:.1f}%)")
